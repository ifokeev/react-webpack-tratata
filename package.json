{
  "name": "react-webpack-tratata",
  "version": "0.0.1",
  "description": "Webpack boilerplate for ReactJS client+server",
  "scripts": {
    "install-prod": "npm install --only=production",
    "dev": "NODE_ENV=development webpack-dev-server --inline --hot --progress --config webpack-client.config.babel.js",
    "build-client": "NODE_ENV=production webpack -p --progress --config webpack-client.config.babel.js",
    "build-server": "NODE_ENV=production webpack -p --progress --config webpack-server.config.babel.js",
    "build": "rm -rf build/* && npm run build-client && npm run build-server",
    "server": "NODE_ENV=production node server.js",
    "babel-version": "babel --version",
    "babel-node-version": "babel-node --version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifokeev/react-webpack-tratata"
  },
  "keywords": [
    "reactjs",
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifokeev/react-webpack-tratata"
  },
  "homepage": "",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-lodash": "^3.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.9.2",
    "lost": "^7.1.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "express": "^4.14.0",
    "react": "^15.3.0",
    "react-dom": "^15.1.0",
    "sass-mq": "^3.2.9",
    "serialize-javascript": "^1.3.0",
    "superagent": "^2.0.0"
  }
}
